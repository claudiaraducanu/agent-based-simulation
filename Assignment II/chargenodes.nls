to charge-nodes-go
  charge-node-agv-on 
end
 
to setup-chargenodes
  set pcolor 27
  set shape "lightning"
  set color black
  set size 0.8
  
  set reserved false
  set available true
end
 
to charge-node-agv-on 
  let patches-on-and-above-me patch-set patch-here (patch-at-heading-and-distance 0 1) (patch-at-heading-and-distance 0 2) 
  ifelse any? agvs-on patches-on-and-above-me[ 
    set available false 
  ][ 
    set available true 
  ] 
end
